model User {
    id: int
    username: string
    name: string
    surname: string?
}

model Group {
    name: string
    users: list[User]
}

rpc GetUser(
    userId: int,
) User

rpc ListUsers() list[User]

rpc CreateUser(
    name: string,
    surname: string?,
) User

rpc GetUsernameMap() map[User]

rpc FindGroupByName(
    name: string,
) Group
