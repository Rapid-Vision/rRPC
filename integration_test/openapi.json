  "components": {
    "schemas": {
      "EmptyModelModel": {
        "type": "object",
        "properties": {
        }
      },
      "TextModelModel": {
        "type": "object",
        "properties": {
          "title": {"nullable":true,"type":"string"},
          "body": {"type":"string"}
        },
        "required": ["body"]
      },
      "FlagsModelModel": {
        "type": "object",
        "properties": {
          "enabled": {"type":"boolean"},
          "retries": {"format":"int32","type":"integer"},
          "labels": {"items":{"type":"string"},"type":"array"},
          "meta": {"additionalProperties":{"type":"string"},"type":"object"}
        },
        "required": ["enabled","retries","labels","meta"]
      },
      "NestedModelModel": {
        "type": "object",
        "properties": {
          "text": {"$ref":"#/components/schemas/TextModelModel"},
          "flags": {"allOf":[{"$ref":"#/components/schemas/FlagsModelModel"}],"nullable":true},
          "items": {"items":{"$ref":"#/components/schemas/TextModelModel"},"type":"array"},
          "lookup": {"additionalProperties":{"$ref":"#/components/schemas/TextModelModel"},"type":"object"}
        },
        "required": ["text","items","lookup"]
      },
      "TestEmptyParams": {
        "type": "object",
        "properties": {
        }
      },
      "TestEmptyResult": {
        "type": "object",
        "properties": {
          "empty_model": {"$ref":"#/components/schemas/EmptyModelModel"}
        }
      },
      "TestBasicParams": {
        "type": "object",
        "properties": {
          "text": {"$ref":"#/components/schemas/TextModelModel"},
          "flag": {"type":"boolean"},
          "count": {"format":"int32","type":"integer"},
          "note": {"nullable":true,"type":"string"}
        },
        "required": ["text","flag","count"]
      },
      "TestBasicResult": {
        "type": "object",
        "properties": {
          "text_model": {"$ref":"#/components/schemas/TextModelModel"}
        }
      },
      "TestListMapParams": {
        "type": "object",
        "properties": {
          "texts": {"items":{"$ref":"#/components/schemas/TextModelModel"},"type":"array"},
          "flags": {"additionalProperties":{"type":"string"},"type":"object"}
        },
        "required": ["texts","flags"]
      },
      "TestListMapResult": {
        "type": "object",
        "properties": {
          "nested_model": {"$ref":"#/components/schemas/NestedModelModel"}
        }
      },
      "TestOptionalParams": {
        "type": "object",
        "properties": {
          "text": {"allOf":[{"$ref":"#/components/schemas/TextModelModel"}],"nullable":true},
          "flag": {"nullable":true,"type":"boolean"}
        }
      },
      "TestOptionalResult": {
        "type": "object",
        "properties": {
          "flags_model": {"$ref":"#/components/schemas/FlagsModelModel"}
        }
      },
      "TestValidationErrorParams": {
        "type": "object",
        "properties": {
          "text": {"$ref":"#/components/schemas/TextModelModel"}
        },
        "required": ["text"]
      },
      "TestValidationErrorResult": {
        "type": "object",
        "properties": {
          "text_model": {"$ref":"#/components/schemas/TextModelModel"}
        }
      },
      "TestUnauthorizedErrorParams": {
        "type": "object",
        "properties": {
        }
      },
      "TestUnauthorizedErrorResult": {
        "type": "object",
        "properties": {
          "empty_model": {"$ref":"#/components/schemas/EmptyModelModel"}
        }
      },
      "TestForbiddenErrorParams": {
        "type": "object",
        "properties": {
        }
      },
      "TestForbiddenErrorResult": {
        "type": "object",
        "properties": {
          "empty_model": {"$ref":"#/components/schemas/EmptyModelModel"}
        }
      },
      "TestNotImplementedErrorParams": {
        "type": "object",
        "properties": {
        }
      },
      "TestNotImplementedErrorResult": {
        "type": "object",
        "properties": {
          "empty_model": {"$ref":"#/components/schemas/EmptyModelModel"}
        }
      }
      ,
      "RPCError": {
        "type": "object",
        "properties": {
          "type": {
            "type": "string",
            "enum": [
              "custom",
              "validation",
              "input",
              "unauthorized",
              "forbidden",
              "not_implemented"
            ]
          },
          "message": {
            "type": "string"
          }
        },
        "required": ["type", "message"]
      }
    }
  }
